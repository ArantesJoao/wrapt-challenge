generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model File {
  id        String   @id @default(uuid()) @map("_id")
  name      String
  type      String
  size      String
  path      String
  serverUrl String
  createdAt DateTime @default(now())
}
